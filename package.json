    "jquery": "https://github.com/jquery/jquery#2.2.4",
    "jquery-ui": "^1.10.5",
    "morgan": "https://github.com/expressjs/morgan#1.6.1",
    "node-uuid": "https://github.com/broofa/node-uuid#v1.4.7",
